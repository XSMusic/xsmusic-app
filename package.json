{
  "name": "xsmusic-app",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "set-env": "ts-node ./scripts/setenv.ts",
    "start": "npm run set-env -- --environment=dev && ng serve --host 0.0.0.0",
    "build": "npm run set-env -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "run:android": "npm run build && npx cap sync && npx capacitor run android",
    "run:note": "npm run build && npx cap sync && npx capacitor run android --target R5CN80DTHJP",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,css,ts,html}\" || ECHO .",
    "prettier": "prettier --write \"./{e2e,src}/**/*.{scss,css,ts,html}\"",
    "pretty-quick": "pretty-quick --staged",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "ng test --code-coverage",
    "test:no-watch": "ng test --no-watch --code-coverage",
    "prepare": "husky install",
    "dev:ssr": "ng run xsmusic:serve-ssr",
    "serve:ssr": "node dist/xsmusic/server/main.js",
    "build:ssr": "npm run build && ng run xsmusic:server",
    "prerender": "ng run xsmusic:prerender",
    "release": "standard-version",
    "bundle-report": "npm run build -- --stats-json && webpack-bundle-analyzer dist/xsmusic/browser/stats.json"
  },
  "description": "xsmusic-app",
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.2",
    "@angular/common": "^15.0.2",
    "@angular/compiler": "^15.0.2",
    "@angular/core": "^15.0.2",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.0.2",
    "@angular/platform-browser": "^15.0.2",
    "@angular/platform-browser-dynamic": "^15.0.2",
    "@angular/platform-server": "^15.0.2",
    "@angular/router": "^15.0.2",
    "@angular/service-worker": "^15.0.2",
    "@angular/youtube-player": "^15.0.2",
    "@capacitor/android": "^4.3.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "^4.0.1",
    "@capacitor/core": "latest",
    "@capacitor/geolocation": "^4.0.1",
    "@capacitor/ios": "^4.3.0",
    "@capacitor/share": "^4.1.0",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@nguniversal/express-engine": "^15.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "angular-svg-icon": "^14.0.0",
    "chart.js": "^3.6.0",
    "chartjs": "0.3.24",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.15.2",
    "jest": "^29.2.1",
    "leaflet": "^1.9.2",
    "leaflet.markercluster": "^1.5.3",
    "localstorage-polyfill": "^1.0.1",
    "ng2-charts": "4.1.1",
    "ngx-google-analytics": "^14.0.1",
    "ngx-permissions": "^14.0.0",
    "prettier": "^2.7.1",
    "rxjs": "~7.5.0",
    "swiper": "^8.4.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.2",
    "@capacitor/cli": "latest",
    "@nguniversal/builders": "^15.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.2.0",
    "@types/leaflet": "^1.9.0",
    "@types/leaflet-routing-machine": "^3.2.4",
    "@types/leaflet.markercluster": "^1.5.1",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.46.0",
    "autoprefixer": "^10.4.13",
    "compression": "~1.7.4",
    "eslint": "~7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^8.0.2",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "eslintIgnore": [
    "*.spec.ts"
  ],
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}