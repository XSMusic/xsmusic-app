{
  "name": "xsmusic-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "set-env": "ts-node ./scripts/setenv.ts",
    "start": "npm run set-env -- --environment=dev && ng serve",
    "build": "npm run set-env -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "run:android": "ng build && npx capacitor run android",
    "run:note": "ng build && npx capacitor run android --target R5CN80DTHJP",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,css,ts,html}\" || ECHO .",
    "prettier": "prettier --write \"./{e2e,src}/**/*.{scss,css,ts,html}\"",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "ng test"
  },
  "description": "xsmusic-app",
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@capacitor/android": "^4.3.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "^4.0.1",
    "@capacitor/core": "latest",
    "@capacitor/geolocation": "^4.0.1",
    "@capacitor/ios": "^4.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "angular-svg-icon": "^14.0.0",
    "dotenv": "^16.0.3",
    "jest": "^29.2.1",
    "leaflet": "^1.9.2",
    "leaflet-routing-machine": "^3.2.12",
    "ngx-infinite-scroll": "^14.0.0",
    "ngx-permissions": "^14.0.0",
    "ngx-spinner": "^14.0.0",
    "prettier": "^2.7.1",
    "rxjs": "~7.5.0",
    "swiper": "^8.4.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "~14.2.2",
    "@angular/compiler-cli": "^14.2.0",
    "@capacitor/cli": "latest",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.2.0",
    "@types/leaflet": "^1.9.0",
    "@types/leaflet-routing-machine": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "autoprefixer": "^10.4.13",
    "eslint": "~7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.2"
  },
  "eslintIgnore": [
    "*.spec.ts"
  ],
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
